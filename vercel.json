{
  "version": 2,
  "builds": [
    {
      "src": "./api/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/(.*)",
      "dest": "/api"
    },
    {
      "src": "/(.*)",
      "dest": "/api/app.py"
    },
    {
      "src": "/api", "dest": "/api/app.py"
    }
  ]
}